{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "SwaggerEndPoints": [
    {
      "Key": "shifttrack-api",
      "Config": [
        {
          "Name": "ShiftTrack API",
          "Version": "1.0",
          "Url": "http://shifttrack-api:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "user-authentication-api",
      "Config": [
        {
          "Name": "User authentication service",
          "Version": "1.0",
          "Url": "http://user-authentication-api:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "generators-pdf-api",
      "Config": [
        {
          "Name": "Generators Pdf service",
          "Version": "1.0",
          "Url": "http://generators-pdf-api:8080/swagger/v1/swagger.json"
        }
      ]
    }
  ],

  "Routes": [
    {
      "SwaggerKey": "shifttrack-api",
      "DownstreamPathTemplate": "/api/shift-track/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "shifttrack.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/shift-track/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ]
    },
    {
      "SwaggerKey": "user-authentication-api",
      "DownstreamPathTemplate": "/api/user/authentication/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "user.authentication.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/user/authentication/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ]
    },
    {
      "SwaggerKey": "generators-pdf-api",
      "DownstreamPathTemplate": "/api/generators/pdf/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "generators.pdf.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/generators/pdf/{everything}",
      "UpstreamHttpMethod": [ "POST" ]
    }
  ]
}
